
public class Tubing {
	String name;
	String bendable; 
	String diameter; 
	
	public Tubing(String name, String bendable, String diameter) {
		this.name = name; 
		this.bendable = bendable; 
		this.diameter = diameter; 
		
	}

	public String getName() {
		return name; 
	}
	public String getBendable() {
		return bendable;
	}
	public String getDiameter() {
		return diameter;
	}

}
