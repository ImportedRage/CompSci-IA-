
public class Pump {
	String name; 
	String type;
	String brand;
	
	public Pump(String name, String type, String brand) {
		this.name = name; 
		this.type = type; 
		this.brand = brand; 
	
	
	}
	
	public String getName() {
		return name; 
	}
	public String getType() {
		return type;
	}
	public String getBrand() {
		return brand;
	}

}
